<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity.MainActivity">


    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:gravity="center_horizontal|center_vertical"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="ChamFP"
            app:srcCompat="@drawable/phi" />

        <Button
            android:id="@+id/btnFP"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="ChamFP"
            android:text="Calcular Banco de Capacitores" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="match_parent"
            android:layout_height="50dp" />

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="ChamarDeseq"
            app:srcCompat="@drawable/desq" />

        <Button
            android:id="@+id/btnDesequilibrio"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="ChamarDeseq"
            android:text="Calcular Desequilíbrio de Tensão" />
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>